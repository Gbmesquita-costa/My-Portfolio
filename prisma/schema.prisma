generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Donations {
  id         String   @id @default(uuid())
  name       String
  message    String
  amount     Int
  
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt()

  @@map("donations")
}